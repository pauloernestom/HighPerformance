        <%- include('partials/header') %>
        <link rel=”canonical” href="https://highperformance.herokuapp.com/pages/cadastro/cadastro.html" />
        <link rel="stylesheet" href="/css/cadastro.css">
    </head>
<body>
    <%- include('partials/navBar') %>
    <%- include('partials/bikeGif') %>    

    <div class="logout">
        <div class="row">
            <div class="col s12 m12 l5" id="cadastroContainer">
                <!-- <a href="/">
                    <img class="responsive-img logo" src="/img/logo.png" alt="High Performance">
                </a> -->
                <h1>Cadastro</h1>
                <p>Faça seu cadastro, entre na plataforma e monitore seus alunos.</p>               
            </div>

            <div class="col s12 m12 l7" id="containerForm">
                <form id="formCad" action="/cadastro" method="POST">
                    <div class="input-field">
                        <i class="material-icons prefix">account_circle</i>
                        <input name="name" id="name" type="text" data-error=".errorTxt1">
                        <label for="name">Nome</label>
                        <div class="errorTxt1 right-align "></div>
                    </div>
                    <div class="input-field">
                        <i class="material-icons prefix">email</i>
                        <input name="email" id="email" type="email" data-error=".errorTxt2">
                        <label for="email">E-mail</label>
                        <div class="errorTxt2 right-align "></div>
                    </div>
                    <div class="input-field">
                        <i class="material-icons prefix">call</i>
                        <input name="cel" attrname="telephone1" type="tel" id="tel" data-error=".errorTxt3">
                        <label for="cel">Celular</label>
                        <div class="errorTxt3 right-align "></div>
                    </div>
                    <div class="input-field">
                        <i class="material-icons prefix">lock_outline</i>
                        <input name="senha" id="senha" type="password" data-error=".errorTxt4">
                        <label for="senha">Senha</label>
                        <div class="errorTxt4 right-align "></div>
                    </div>
                    <div class="input-field">
                        <i class="material-icons prefix">lock_outline</i>
                        <label for="repSenha"> Repetir Senha</label>
                        <input name="repSenha" id="repSenha" type="password" data-error=".errorTxt5">                        
                        <div class="errorTxt5 right-align "></div>
                    </div>
                    <div class="input-field">
                        <button class="btn waves-effect waves-light right submit" type="submit" name="action">Cadastrar</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <%- include('partials/footer') %>