        <%- include('partials/header') %>
        <link rel="stylesheet" href="/css/alunos.css">
        <link rel=”canonical” href="https://highperformancedh.azurewebsites.net"/>
    </head>
<body>

    <%- include('partials/navBar') %>
    <%- include('partials/bikeGif') %>
 
    <!-- Alunos -->
    <div class="container">
        <div class="col s12 m12 l12 quadrado">
            <div id="alunos"><h2>Alunos ativos</h2></div>                                    
        </div>
        <div class="col s12 m12 l12 baixoQuadrado">
            <i class="material-icons">person_add</i>
            <a id="addAluno" href="/home/cadastrarAlunos">ADICIONAR NOVO ALUNO</a>          
        </div>

        <!-- INICIO DA LISTA -->
        <div class="container" id="lista">
            <div class="row">
            <div class="col s12 m12 l12 left-align">        
                <div class="nav-wrapper">
                    <form action="/busca" method="get" id="formBuscaAluno">                        
                        <div class="input-field">
                            <input id="search" type="text" name="q" placeholder="Pesquisar Alunos" autocomplete="off">
                            <!-- <i class="material-icons">close</i> -->
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

        <br/>
        <!-- Lista dos alunos -->
        
        <div class="col s12 m12 l12">
            <table class="highlight centered table" id="tabledata">
                <thead>
                    <tr>
                        <th>Imagem</th>
                        <th>Nome</th>
                        <th>Telefone</th>
                        <th>Meta</th>
                    </tr>
                </thead>
                
                <tbody>
                    <% for (aluno of alunos) { %>
                    <tr data-url="/home/treino/<%= aluno.id %>">
                        <td><img id="foto-aluno" src="<%= aluno.img %>" alt="Foto <%= aluno.nome %>" width="130" height="130"></td>
                        <td><%= aluno.nome %></td>
                        <td><%= aluno.telefone %></td>
                        <td><%= aluno.meta%> km</td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>


        <div class="row">
            <div class="col l12 m12 s12 center-align">
                <ul id="dropdown2" class="dropdown-content">
                    <li>
                        <a onclick="dowloadDoc()" id="pdf" class="black-text">PDF</a>
                    </li>
                    <li>
                        <a href="#!" id="csv" class="black-text">Excel</a>
                    </li>
                </ul>
                <a class="btn dropdown-trigger" href="#!" data-target="dropdown2" id="btnExport">Exportar para<i class="material-icons right" >arrow_drop_down</i></a>
            </div>
        </div>       
        
    </div>
    
    <%- include('partials/footer') %>
    <%- include('partials/rodape') %>
   