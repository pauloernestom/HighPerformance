        <%- include('partials/header') %>
        <link rel=”canonical” href="https://highperformance.herokuapp.com/pages/financas/financas.html" />
        <link rel="stylesheet" href="/css/financas.css">
    </head>
<body>
    
    <%- include('partials/navBar') %>
    <%- include('partials/bikeGif') %> 


    <h1>oi <%=financa.jane%></h1>
    <div class="row" id="grafico">
        <div class="col s12 m12 l8">
            <canvas id="myChart" width="400" height="300"></canvas>
        </div>

        <div class="col s12 m12 l4">
            <div class="cards">

                <div class="card">
                    <div>
                        <i class="material-icons background-round mt-5">attach_money</i>
                    </div>
                    <div class="right-align">
                        <h5 class="white-text">R$ 8.900,00</h5>
                        <p class="white-text">Total</p>
                    </div>
                </div>
                <div class="card2">
                    <div>
                        <i class="material-icons background-round mt-5">timeline</i>
                    </div>
                    <div class="right-align">
                        <h5 class="white-text">%10</h5>
                        <p class="white-text">Crescimento</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col s12 m12 l6">
        <form id="form" id="form" action="?_method=PUT" method="post">
                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="jan" id="jan" type="number" data-error=".errorTxt1" value="1">
                    <label for="jan">Janeiro</label>
                    <div class="errorTxt1 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="fev" id="fev" type="number" data-error=".errorTxt2" value="">
                    <label for="fev">Fevereiro</label>
                    <div class="errorTxt2 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="mar" id="mar" type="number" data-error=".errorTxt3" value="">
                    <label for="mar">Março</label>
                    <div class="errorTxt3 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="abr" id="abr" type="number" data-error=".errorTxt4">
                    <label for="abr">Abril</label>
                    <div class="errorTxt4 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="mai" id="mai" type="number" data-error=".errorTxt5" value="">
