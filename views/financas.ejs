                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="jun" id="jun" type="number" data-error=".errorTxt6" value="">
                    <label for="jun">Junho</label>
                    <div class="errorTxt6 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="jul" id="jul" type="number" data-error=".errorTxt7" value="">
                    <label for="jul">Julho</label>
                    <div class="errorTxt7 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="ago" id="ago" type="number" data-error=".errorTxt8" value="">
                    <label for="ago">Agosto</label>
                    <div class="errorTxt8 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="set" id="set" type="number" data-error=".errorTxt9" value="">
                    <label for="set">Setembro</label>
                    <div class="errorTxt9 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="out" id="out" type="number" data-error=".errorTxt10" value="">
                    <label for="out">Outubro</label>
                    <div class="errorTxt10 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="nov" id="nov" type="number" data-error=".errorTxt11" value="">
                    <label for="nov">Novembro</label>
                    <div class="errorTxt11 right-align"></div>
                </div>

                <div class="input-field">
                    <i class="material-icons prefix">monetization_on</i>
                    <input name="dez" id="dez" type="number" data-error=".errorTxt12" value="">
                    <label for="dez">Dezembro</label>
                    <div class="errorTxt12 right-align"></div>
                </div>
                
                    <div class="col s12 m6 l6">
                        <div class="input-field">
                            <button id="btn" class="btn waves-effect waves-light right submit" type="submit" name="action">Salvar</button>
                        </div>
                </div>
            </form>
    </div>

    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js"></script>
    <script src="/js/finacas.js"></script>

    <%- include('partials/footer') %>
    <script>
        var ctx = document.getElementById("myChart").getContext("2d");
var myChart = new Chart(ctx, {
    type: "bar",
    data: {
        labels: [
            "Janeiro",
            "Fevereiro",
            "Março",
            "Abril",
            "Maio",
            "Junho",
            "Julho",
            "Agosto",
            "Setembro",
            "Outubro",
            "Novembro",
            "Dezembro",
        ],
        datasets: [{
            label: "Pagamentos",
            data: [12.20, 22, 3, 5, 2, 3, 12, 22, 3, 5, 2, 3, 1],
            backgroundColor: [
                "rgba(255, 99, 132, 0.2)",
                "rgba(54, 162, 235, 0.2)",
                "rgba(255, 206, 86, 0.2)",
                "rgba(75, 192, 192, 0.2)",
                "rgba(153, 102, 255, 0.2)",
                "rgba(255, 159, 64, 0.2)",
                "rgba(112, 128, 144, 0.2)",
                "rgba(0, 225, 0, 0.2)",
                "rgba(128, 0, 128, 0.2)",
                "rgba(178, 34, 34, 0.2)",
                "rgba(107, 66, 38, 0.2)",
                "rgba(255, 255, 0, 0.2)",
            ],
            borderColor: [
                "rgba(255, 99, 132, 1)",
                "rgba(54, 162, 235, 1)",
                "rgba(255, 206, 86, 1)",
                "rgba(75, 192, 192, 1)",
                "rgba(153, 102, 255, 1)",
                "rgba(255, 159, 64, 1)",
                "rgba(112, 128, 144, 1)",
                "rgba(0, 225, 0, 1)",
                "rgba(128, 0, 128, 1)",
                "rgba(178, 34, 34, 1)",
                "rgba(107, 66, 38, 1)",
                "rgba(255, 255, 0, 1)",
            ],
            borderWidth: 1,
        }, ],
    },
    options: {
        title: {
            display: true,
            fontSize: 20,
            text: "PAGAMENTOS MENSAIS",
        },
        labels: {
            fontStyle: "bold",
        },
        scales: {
            yAxes: [{
                ticks: {
                    // Include a dollar sign in the ticks
                    callback: function(value, index, values) {
                        return "R$ " + value;
                    },
                },
            }, ],
        },
    },
});
    </script>
    